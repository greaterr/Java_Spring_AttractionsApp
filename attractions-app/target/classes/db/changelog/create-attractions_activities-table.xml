<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd">
    <changeSet id="4" author="Greaterr">
        <createTable tableName="attractions_activities">
            <column name="attraction_id" type="bigint">
                <constraints nullable="false" foreignKeyName="fk_attractions_activities_attraction"
                             references="attractions(attraction_id)"/>
            </column>
            <column name="activity_id" type="bigint">
                <constraints nullable="false" foreignKeyName="fk_attractions_activities_activity"
                             references="activities(activity_id)"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>